      "source": [
        "import numpy as np\n",
        "from scipy.interpolate import interp1d\n",
        "\n",
        "x = np.array([0, 5, 10])\n",
        "y = np.array([10, 15, 20])\n",
        "\n",
        "f = interp1d(x, y, kind='linear')\n",
        "\n",
        "x_target = 7.2\n",
        "y_target = f(x_target)\n",
        "\n",
        "print(f\"Kecepatan lari pada jarak {x_target} meter adalah {y_target:.2f} m/detik\")\n"
      ]
    }
  ]
